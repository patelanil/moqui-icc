<?xml version="1.0" encoding="UTF-8"?>

<entity-facade-xml type="ext-upgrade-v1.6.3">
<!--    Template Service Job to generate the Fulfilled Order Items Feed from HotWax-->
    <moqui.service.job.ServiceJob jobName="generate_FulfilledOrderItemsFeed">
        <parameters parameterName="orderTypeId"/>
    </moqui.service.job.ServiceJob>

    <!-- System Message Type for Fulfilled Order Items Feed for Transfer Order -->
    <moqui.service.message.SystemMessageType systemMessageTypeId="TransferOrderFulfilledItemsFeed"
        description="Generate HotWax Fulfilled Order Items Feed for Transfer Orders"
        parentTypeId="LocalFeedFile"
        sendPath="/home/${sftpUsername}/hotwax/TransferOrderFulfilledItemsFeed/${productStoreId}-TransferOrderFulfilledItemsFeed-${systemMessageId}-${dateTime}.json"
        sendServiceName="co.hotwax.ofbiz.SystemMessageServices.send#SystemMessageFileSftp"
        receivePath="${contentRoot}/hotwax/TransferOrderFulfilledItemsFeed/${productStoreId}-TransferOrderFulfilledItemsFeed-${dateTime}.json"/>

    <!-- Template Service Job to generate Inventory Variance Feed from HotWax -->
    <moqui.service.job.ServiceJob jobName="generate_InventoryVarianceFeed">
        <parameters parameterName="customParametersMap"/>
    </moqui.service.job.ServiceJob>
</entity-facade-xml>
